{
    "name": "masroore/sparky-stripe",
    "description": "Sparky provides scaffolding for Laravel SaaS applications.",
    "keywords": ["laravel", "stripe", "billing", "scaffolding", "saas"],
    "license": "MIT",
    "require": {
        "php": "^8.1|^8.2",
        "ext-json": "*",
        "dompdf/dompdf": "^2.0",
        "illuminate/contracts": "^9.0|^10.0",
        "inertiajs/inertia-laravel": "^0.6.9",
        "laravel/cashier": "^14.12",
        "mpociot/vat-calculator": "^3.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^8.5",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "Spark\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spark\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        },
        "laravel": {
            "providers": [
                "Spark\\SparkServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
